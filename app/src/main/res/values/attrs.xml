<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomUnitView">
        <attr name="defaultValue" format="float"/>
        <attr name="changeFactor" format="float"/>
        <attr name="unitTypes" format="enum">
            <enum name="minutes" value="0"/>
            <enum name="hours" value="1"/>
            <enum name="kg" value="2"/>
            <enum name="lbs" value="3"/>
        </attr>
    </declare-styleable>
</resources>
